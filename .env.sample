# .env.example

# Your Pushover User Key
PUSHOVER_USER=put_your_user_key_here

# Your Pushover Application API Token
PUSHOVER_TOKEN=put_your_app_token_here

# Solana Write Endpoint (single endpoint for write operations)
SOLANA_WRITE_ENDPOINT=https://your-write-endpoint-url-here

# Solana Read Endpoints (comma-separated list of name:url pairs)
# Format: Name 1:https://url1,Name 2:https://url2,Name 3:https://url3
SOLANA_READ_ENDPOINTS=Endpoint 1:https://your-read-endpoint-1,Endpoint 2:https://your-read-endpoint-2,Endpoint 3:https://your-read-endpoint-3
